public class Pair<K, V> {
    private K key;
    private V value;

    public Pair(K key, V value) {
        this.key = key;
        this.value = value;
    }

    public K getKey() {
        return key;
    }

    public V getValue() {
        return value;
    }
}
public class Main {
    public static void main(String[] args) {
        
        Pair<Integer, String> intStrPair = new Pair<>(10, "Hello");
        System.out.println("Key: " + intStrPair.getKey() + ", Value: " + intStrPair.getValue());

        
        Pair<Double, Boolean> doubleBoolPair = new Pair<>(3.14, true);
        System.out.println("Key: " + doubleBoolPair.getKey() + ", Value: " + doubleBoolPair.getValue());
    }
}
