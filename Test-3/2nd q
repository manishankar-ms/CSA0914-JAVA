public class RightAngleTriangle {
    public static void main(String[] args) {
        int numRows = 5; // number of rows in the triangle
        for (int i = 1; i <= numRows; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }
}
