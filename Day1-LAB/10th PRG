public class GreatestIfStatement {
    public static void main(String[] args) {
       
        int if1 = 10;
        int if2 = 20;
        int if3 = 30;

     
        if (if1 > if2) {
            if (if1 > if3) {
                System.out.println("If statement 1 contains the greatest value: " + if1);
            } else {
                System.out.println("If statement 3 contains the greatest value: " + if3);
            }
        } else {
            if (if2 > if3) {
                System.out.println("If statement 2 contains the greatest value: " + if2);
            } else {
                System.out.println("If statement 3 contains the greatest value: " + if3);
            }
        }
    }
}
